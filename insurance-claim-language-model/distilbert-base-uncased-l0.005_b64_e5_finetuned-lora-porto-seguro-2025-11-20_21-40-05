      "outputs": [],
      "source": [
        "def pandas_to_dataset(df):\n",
        "  instruction_data = df.apply(create_text_classification_example, axis=1).tolist()\n",
        "  df_instructions = pd.DataFrame(instruction_data)\n",
        "  hf_dataset = Dataset.from_pandas(df_instructions)\n",
        "  return hf_dataset"
      ]
    },
